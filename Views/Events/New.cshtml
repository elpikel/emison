@model Emison.ViewModels.NewEvent
@{
    ViewData["Title"] = "Event";
}

<div class="text-center">
    <form asp-controller="Events" asp-action="New" method="post">
      <div>
        <label asp-for="Date"></label>
        <input asp-for="Date"/>
      </div>
      <div>
        <label asp-for="Place"></label>
        <input asp-for="Place"/>
      </div>
      <button type="submit">Create</button>
    </form>
</div>